@model List<Slider>


<a href="/admin/slider/create" class="btn btn-dark">Create</a>
<table class="table table-bordered table-striped table-hover">
    <thead>
        <tr>
            <th>#</th>
            <th>Title</th>
            <th>Sub</th>
            <th>Image</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td>@item.Description</td>
                <td>
                    <img src="@item.PhotoUrl" width="200" alt="" />
                </td>
                <td>
                    <a href="/admin/slider/delete/@item.Id" class="btn btn-danger">Delete</a>
                    <a href="/admin/slider/edit/@item.Id" class="btn btn-warning">Edit</a>
                </td>
            </tr>
        }
    </tbody>
</table>